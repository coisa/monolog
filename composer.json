{
    "name": "coisa/monolog",
    "description": "PSR-11 Monolog container config provider",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "monolog",
        "logger",
        "psr-3",
        "container",
        "psr-11",
        "middleware",
        "psr-15",
        "expressive",
        "zf",
        "zendframework",
        "pimple",
        "servicemanager",
        "phalcon"
    ],
    "authors": [
        {
            "name": "Felipe Say√£o Lobato Abreu",
            "email": "github@felipeabreu.com.br"
        }
    ],
    "support": {
        "issues": "https://github.com/coisa/monolog/issues",
        "source": "https://github.com/coisa/monolog",
        "rss": "https://github.com/coisa/monolog/releases.atom"
    },
    "config": {
        "sort-packages": true,
        "prefer-stable": true,
        "optimize-autoloader": true
    },
    "require": {
        "php": "^7.1",
        "psr/log": "^1.0",
        "psr/container": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "monolog/monolog": "^1.24",
        "zendframework/zend-config-aggregator": "^1.1",
        "gitonomy/gitlib": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "league/container": "^3.2",
        "phpunit/phpunit": "^7.3",
        "rollbar/rollbar": "^1.5",
        "sensiolabs/security-checker": "^5.0",
        "sentry/sentry": "^1.9",
        "squizlabs/php_codesniffer": "^3.3",
        "zendframework/zend-pimple-config": "^1.1",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-stdlib": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "CoiSA\\Monolog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoiSA\\Monolog\\Test\\": "tests/"
        }
    },
    "extra": {
        "zf": {
            "component": "CoiSA\\Monolog",
            "config-provider": "CoiSA\\Monolog\\ConfigProvider"
        },
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "suggest": {
        "zendframework/zend-pimple-config": "If you intend to use with any of follow: Pimple\\Container or League\\Container"
    },
    "scripts": {
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix"
    }
}
