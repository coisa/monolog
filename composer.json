{
    "name": "coisa/monolog",
    "description": "PSR-11 Monolog container config provider",
    "license": "Apache-2.0",
    "type": "library",
    "keywords": [
        "monolog",
        "logger",
        "psr-3",
        "container",
        "psr-11",
        "middleware",
        "psr-15",
        "expressive",
        "zf",
        "zendframework",
        "pimple",
        "servicemanager",
        "phalcon"
    ],
    "authors": [
        {
            "name": "Felipe Say√£o Lobato Abreu",
            "email": "contato@felipeabreu.com.br"
        }
    ],
    "support": {
        "issues": "https://github.com/coisa/monolog/issues",
        "source": "https://github.com/coisa/monolog",
        "rss": "https://github.com/coisa/monolog/releases.atom"
    },
    "require": {
        "php": "^7.1",
        "psr/log": "^1.0",
        "psr/container": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "monolog/monolog": "^1.23",
        "zendframework/zend-config-aggregator": "^1.1"
    },
    "require-dev": {
        "rollbar/rollbar": "^1.5",
        "sentry/sentry": "^1.9",
        "zendframework/zend-pimple-config": "^1.1",
        "zendframework/zend-servicemanager": "^3.3",
        "league/container": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "CoiSA\\Monolog\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggest": {
        "zendframework/zend-pimple-config": "If you intend to use with any of follow: Pimple\\Container, League\\Container or Phalcon\\Di"
    }
}
